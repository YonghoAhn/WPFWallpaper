<Page x:Class="WPFWallpaper.Pages.SettingPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPFWallpaper.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="333" d:DesignWidth="640"
      Title="SettingPage">

    <StackPanel>
        <CheckBox Name="chkStartup" HorizontalAlignment="Left" Margin="10,10,0,0" Content="Start When Windows Start" VerticalAlignment="Top" Foreground="White" Checked="CheckBox_Checked" Unchecked="chkStartup_Unchecked"/>
        <StackPanel Margin="10,10,0,0">
            <Label Content="Made by - Team Radionoise" Foreground="White" />
            <Label Content="Tel - 010 - 3702 - 0628" Foreground="White" />
            <Label Content="Ver - 1.0.0" Foreground="White" />
            <Label Content="Copyright - Team Radionoise" Foreground="White" />
        </StackPanel>
    </StackPanel>
</Page>
